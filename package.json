{
  "name": "cypress_v9",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-headed": "npx cypress run --headed",
    "triggerAllTests-chrome": "npx cypress run --browser chrome",
    "testAlias": "npx cypress run --spec cypress/integration/automation-test-store/alias-invoke.js",
    "triggerAllTests-dashboard": "npx cypress run --record --key ba9f1441-98b9-42e0-ac4f-50591b3c9e79",
    "triggerAllTests-webdriveruni": "npx cypress run --spec cypress/integration/webdriver-uni/*",
    "triggerAllTests-automationteststore": "npx cypress run --spec cypress/integration/automation-test-store/*",
    "junit-merge": "npx junit-merge -d cypress/results/junit -o cypress/results/junit/results.xml",
    "delete-junit-report": "rimraf cypress/results/junit/results.xml",
    "delete-results": "rimraf cypress/results/* || true",
    "mochawesome-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json",
    "delete-mochawesome-report": "rm -rf mochawesome-report/* || true",
    "cypress-regression-pack": "npm run delete-results && npm run delete-mochawesome-report && npm run triggerAllTests-headless && npm run mochawesome-merge",
    "cypress-regression-automationteststore": "npm run delete-results && npm run delete-mochawesome-report && npm run triggerAllTests-automationteststore && npm run mochawesome-merge",
    "cypress-one-test": "npm run delete-results && npm run delete-mochawesome-report && npm run testAlias && npm run mochawesome-merge"
  },
  "author": "Albert Vieira",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.6.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-xpath": "^1.6.2",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}
